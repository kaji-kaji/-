import java.util.*;

public class Main {
    public static void main(String[] args) throws Exception {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int a[] = new int[n];
        
        for(int i = 0; i < n; i++){
            a[i] = i;
        }
        
        int s = a[0];
        
        for(int i = 1; i < n; i++){
            a[i-1] = a[i]; 
        }
        
        a[n-1] = s;
        
        for(int i = 0; i < n; i++){
            System.out.print(" " + a[i]);
        }
        
    }
}



#include <stdio.h>
#include <math.h>
double f(double x) { return 3 * x * x + 4; }
int main() {
double x0 = 10;
double err_bound = 0.0001;
double grad = 0;//反復を終了する更新量の閾値
double gradold = 0;
int n = 1;
int n_init = n;
printf("%10s%15s%15s¥n", "#Division", "Trapezoid", "DiffUpdate");
  
do {
  gradold = grad;
  double h = 1.0/n;
  grad = f((x0 + h) - f(x0)) / h;
  printf("%10d%15.3f", n, grad);
  n = n * 2;
} while (fabs(grad - gradold) > err_bound ); //差が一定値より大きい or 初回は継続
}
