import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int m = sc.nextInt();
        int[] a = new int[100];
        int i = 0;
        while(n > 0){
            a[i] = n % m;
            n = n / m;
            i++;
        }
        
        for(int j = 0; j < i; j++)
        System.out.print(a[j]);
    }
}
